	$scope.nameConvertForURL = function(val){
		if(val == undefined || val == '') val=' ';
		val = val.replace(/\//g, '-');
		val = val.replace(/  /g, '-');
		val = val.replace(/ /g, '-');
		val = val.toLowerCase();
		return val;		
	}
	
	$scope.getMonthFromDate = function(val){
		if(val == undefined || val == '') val='';
		val = val.split(" ");
		if(val[1] != undefined)
			return val[1];
		else
			return '';
		
	}
	$scope.getDayFromDate = function(val){
